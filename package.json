{
  "name": "car-details-api",
  "version": "1.0.0",
  "description": "car details for company and details",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pulkit69/car-detials-poc.git"
  },
  "author": "Pulkit Agrawal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pulkit69/car-detials-poc/issues"
  },
  "homepage": "https://github.com/pulkit69/car-detials-poc#readme",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint src/**/*.ts",
    "typedoc": "typedoc",
    "typings": "typings",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "tsc ",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "npm run build -- --self-study-authorization-api:environment=dev",
    "prebuild:uat": "npm run clean:dist",
    "build:uat": "npm run build -- --self-study-authorization-api:environment:environment=uat",
    "prebuild:prod": "echo prebuild:prod... creating tarball...",
    "build:prod": "npm run build -- --self-study-authorization-api:environment=prod",
    "postbuild:prod": "npm run createarchive",
    "server": "gulp copy-ejs && node dist/src/app/app.js",
    "server:local": "set \"NODE_ENV=local\" && set \"ENABLE_CORS=true\" && node dist/src/app/app.js",
    "server:dev": "set \"NODE_ENV=dev\" && node dist/src/app/app.js",
    "server:uat": "set \"NODE_ENV=uat\" && node dist/src/app/app.js",
    "server:prod": "set \"NODE_ENV=prod\" &&  node dist/src/app/app.js",
    "server:test": "set \"NODE_ENV=test\" &&  node dist/src/app/app.js",
    "lint": "npm run tslint '**/*.ts'",
    "test": "set \"NODE_ENV=test\" && mocha \"dist/spec/**/*spec.js\" --recursive -R spec",
    "istanbul": "istanbul",
    "cover": "set \"NODE_ENV=test\" && istanbul cover --report lcov --report clover ./node_modules/mocha/bin/_mocha \"dist/spec/**/*.spec.js\" -- -R spec",
    "testdeploy": "set \"NODE_ENV=test\" && mocha \"./dist/spec/**/*spec.js\" --recursive -R nyan",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "inspect": "node --inspect --debug-brk dist/src/app/app.js",
    "start": "npm run build && npm run server"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/connect-mongo": "0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/express-session": "^1.15.6",
    "@types/lodash": "^4.14.85",
    "@types/log4js": "^2.3.5",
    "@types/mongoose": "^4.7.27",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.15.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "mongoose": "~4.5.4",
    "path": "^0.12.7",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.0.39",
    "@types/log4js": "^2.3.5",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "@types/q": "^1.0.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mock-express-request": "^0.2.0",
    "mock-express-response": "^0.2.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "ts-node": "^2.1.0",
    "tslint": "^3.8.1",
    "typedoc": "^0.3.12",
    "typescript": "^2.4.2",
    "typings": "^2.1.1"
  }
}
